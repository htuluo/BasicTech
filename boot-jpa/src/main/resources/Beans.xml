<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="helloWorld" class="com.llm.entity.HelloWorld" init-method="init" destroy-method="destory">
        <property name="message" value="Hellog beinging"/>

    </bean>
    <bean class="com.llm.bean.InitHelloWorld"></bean>
    <bean id="cStartEventHandler" class="com.llm.bean.CStartEventHandler"></bean>
    <bean id="cStopEventHandler" class="com.llm.bean.CStopEventHandler"></bean>

</beans>